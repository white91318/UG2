<?xml version="1.0" encoding="utf-8"?>
<AbsoluteLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/feedback_bg"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/feedback_bg"
    >

    <ListView
        android:id="@+id/feedback_listview"
        android:layout_width="325dp"
        android:layout_height="629dp"
        android:layout_x="28dp"
        android:layout_y="110dp"
        android:cacheColorHint="@android:color/transparent">
    </ListView>

    <ImageView
        android:id="@+id/stu_object"
        android:layout_width="275dp"
        android:layout_height="180dp"
        android:layout_x="445dp"
        android:layout_y="85dp"
        android:src="@drawable/keyboard_3" />
    <ImageView
        android:id="@+id/stu_calculate_process"
        android:layout_width="275dp"
        android:layout_height="180dp"
        android:layout_x="445dp"
        android:layout_y="333dp"
        android:src="@drawable/keyboard_3" />

    <ImageView
        android:id="@+id/feedback_process"
        android:layout_width="445dp"
        android:layout_height="270dp"
        android:layout_x="794dp"
        android:layout_y="143dp"
        android:src="@drawable/keyboard_3" />

    <TextView
        android:id="@+id/comment"
        android:layout_width="438dp"
        android:layout_height="114dp"
        android:layout_x="424dp"
        android:layout_y="601dp"
        android:text="TextView"
        android:textSize="18sp" />

    <Button
        android:id="@+id/listen_feedback_audio"
        android:layout_width="61dp"
        android:layout_height="81dp"
        android:layout_x="784dp"
        android:layout_y="468dp"
        android:background="@drawable/audio_listen" />

    <TextView
        android:id="@+id/width_value"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="536dp"
        android:layout_y="275dp"
        android:gravity="center"
        android:text="99.00 (m)"
        android:textColor="#000000"
        android:textSize="20dp" />
    <TextView
        android:id="@+id/top_width_value"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="536dp"
        android:layout_y="55dp"
        android:gravity="center"
        android:text="99.00 (m)"
        android:textColor="#000000"
        android:textSize="20dp"
        android:visibility="gone"/>
    <TextView
        android:id="@+id/hight_value"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="367dp"
        android:layout_y="165dp"
        android:gravity="center"
        android:text="100.00 (m)"
        android:textColor="#000000"
        android:textSize="20dp"
        android:rotation="-90"/>

    <TextView
        android:id="@+id/stu_num"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="825dp"
        android:layout_y="65dp"
        android:text="stu01"
        android:textSize="24dp" />

    <Button
        android:id="@+id/give_feedback_bt"
        android:layout_width="60dp"
        android:layout_height="100dp"
        android:layout_x="880dp"
        android:layout_y="455dp"
        android:background="@drawable/give_feedback_bt" />
    <TextView
        android:id="@+id/title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="54dp"
        android:layout_y="8dp"
        android:textSize="20sp"
        android:text="目前位置: 作業繳交區 / 意見交流區" />

    <ImageView
        android:id="@+id/back_home_mess"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="33dp"
        android:layout_y="28dp"
        android:src="@drawable/back_home_mess2"
        />

    <Button
        android:id="@+id/home_bt"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_x="3dp"
        android:layout_y="0dp"
        android:background="#00000000"/>

</AbsoluteLayout>