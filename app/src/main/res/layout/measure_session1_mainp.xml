<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/measure_two3_1"
    android:orientation="vertical">

    <TextView
        android:layout_width="match_parent"
        android:id="@+id/textView3"
        android:layout_weight="1"
        android:layout_height="120dp" />

    <TableLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_weight="1">

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView
                android:id="@+id/textView5"
                android:layout_height="20dp"
                android:layout_width="80dp" />

            <ImageButton
                android:id="@+id/rectangle_pconcept_but"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="#00000000"
                android:src="@drawable/rectangle_pconcept_but" />

            <ImageButton
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/rr1"
                android:id="@+id/rectangle_pbut1"
                android:background="#00000000"/>

            <ImageButton
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/pmakeup_but1"
                android:id="@+id/pmakeup_but1"
                android:background="#00000000"/>

        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView
                android:id="@+id/textView6"
                android:layout_height="8dp"
                android:layout_width="10dp" />

            <ImageButton
                android:id="@+id/triangle_pconcept_but"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="#00000000"
                android:src="@drawable/triangle_pconcept_but" />

            <ImageButton
                android:id="@+id/triangle_pbut1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="#00000000"
                android:src="@drawable/tt1" />

            <ImageButton
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/pmakeup_but_see_record1"
                android:id="@+id/pmakeup_but_see_record1"
                android:background="#00000000"/>
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView
                android:id="@+id/textView7"
                android:layout_height="8dp"
                android:layout_width="10dp" />

            <ImageButton
                android:id="@+id/parallel_pconcept_but"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="#00000000"
                android:src="@drawable/parallel_pconcept_but" />

            <ImageButton
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/pp1"
                android:id="@+id/parallel_pbut1"
                android:background="#00000000"/>
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView
                android:id="@+id/textView8"
                android:layout_height="8dp"
                android:layout_width="10dp" />

            <ImageButton
                android:id="@+id/diamond_pconcept_but"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/diamond_pconcept_but"
                android:background="#00000000"/>

            <ImageButton
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/dd1"
                android:id="@+id/diamond_pbut1"
                android:background="#00000000"/>
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">


            <TextView
                android:id="@+id/textView9"
                android:layout_height="8dp"
                android:layout_width="10dp" />

            <ImageButton
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/trapezoidal_pconcept_but"
                android:id="@+id/trapezoidal_pconcept_but"
                android:background="#00000000"/>

            <ImageButton
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/ttp1"
                android:id="@+id/trapezoidal_pbut1"
                android:background="#00000000"/>
        </TableRow>
        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <Button
                android:layout_width="90dp"
                android:layout_height="83dp"
                android:id="@+id/previous_but_d1"
                android:layout_x="29dp"
                android:layout_y="458dp"
                android:background="#00000000"
                />

            <TextView
                android:layout_width="35dp"
                android:layout_height="wrap_content"
                android:id="@+id/textView17" />

            <ImageButton
                android:id="@+id/but_see_record"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="#00000000"
                android:src="@drawable/but_see_record" />
        </TableRow>
    </TableLayout>


</LinearLayout>